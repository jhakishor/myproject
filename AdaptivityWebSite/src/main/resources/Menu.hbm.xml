<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
		"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="true" default-lazy="false">

<class 
    name="com.adaptivity.blueprint4it.dao.hibernate.Menu" 
    table="menu">

    <id
        name="id"
        type="java.lang.Long"
        column="id">
        
    <generator class="increment" />
    </id>

    <property
        name="menuName"
        type="java.lang.String"
        column="menuName"
        not-null="true"
        unique="true"
        length="20"
    />
    <property
        name="url"
        type="java.lang.String"
        column="url"
        not-null="true"
        unique="true"
        length="20"
    />
    <property
        name="param"
        type="java.lang.String"
        column="param"
        not-null="true"
        length="20"
    />
     <property
        name="pageName"
        type="java.lang.String"
        column="pageName"
        not-null="true"
        length="20"
    />
    
    <property
        name="parentId"
        type="java.lang.Long"
        column="parentId"
        not-null="true"
        unique="true"
    />
    
</class>
</hibernate-mapping>
